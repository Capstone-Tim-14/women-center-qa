{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$ref": "#/definitions/Welcome",
  "definitions": {
    "Welcome": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "code": {
          "type": "integer"
        },
        "message": {
          "type": "string"
        },
        "data": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Datum"
          }
        }
      },
      "required": [
        "code",
        "data",
        "message"
      ],
      "title": "Welcome"
    },
    "Datum": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "integer"
        },
        "title_job": {
          "$ref": "#/definitions/TitleJob"
        },
        "logo": {
          "type": "string",
          "format": "uri",
          "qt-uri-protocols": [
            "https"
          ]
        },
        "company_name": {
          "$ref": "#/definitions/CompanyName"
        },
        "published_at": {
          "type": "string"
        },
        "location": {
          "$ref": "#/definitions/Location"
        },
        "recomendation": {
          "type": "string",
          "format": "boolean"
        }
      },
      "required": [
        "company_name",
        "id",
        "location",
        "logo",
        "published_at",
        "recomendation",
        "title_job"
      ],
      "title": "Datum"
    },
    "CompanyName": {
      "type": "string",
      "enum": [
        "Gojek",
        "Hilton",
        "Google"
      ],
      "title": "CompanyName"
    },
    "Location": {
      "type": "string",
      "enum": [
        "Jakarta, Indonesia",
        "Bali",
        "yogyakarta"
      ],
      "title": "Location"
    },
    "TitleJob": {
      "type": "string",
      "enum": [
        "Backend Engineer (Golang)- Middle/Senior",
        "Front Office Supervisor",
        "Quality Engineer"
      ],
      "title": "TitleJob"
    }
  }
}
